{
    "compilerOptions": {
        "outDir": "./dist/",
        "sourceMap": true,
        "strict": true,
        "noImplicitReturns": true,
        "noImplicitAny": true,
        "moduleResolution": "node",
        "esModuleInterop": true,
        "target": "es2017",
        "lib": ["es5", "ES2015", "dom"],
        "module": "esnext",
        "allowJs": true,
        "skipLibCheck": true,
    },
    "include": [
        "src"
    ]
}
